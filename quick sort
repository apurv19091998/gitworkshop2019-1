#include<stdio.h>
int part(int a[], int low, int high)
{
    int temp;
    int pivot =a[high];
    int i =(low-1);
    for(int j=low;j<=high-1;j++)
    {
        if(a[j]<=pivot)
        {
            i++;
            temp=a[i];
            a[i]=a[j];
            a[j]=temp;
        }
    }
    temp=a[i+1];
    a[i+1] = a[high];
    a[high]=temp;
    return(i+1);
}
void sort(int a[], int low, int high)
{
    if(low<high)
    {
        int pi=part(a, low, high);
        sort(a, low, pi-1);
        sort(a, pi+1, high);
    }
}
int print(int a[], int n)
{
    for(int i=0;i<n;i++)
    {
        printf("%d\n",a[i]);
    }
}
int main()
{
    int n,i;
    scanf("%d", &n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d", &a[i]);
    }
    sort(a,0,n-1);
    print(a,n);
}
